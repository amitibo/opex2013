@INPROCEEDINGS{agarwal,
  author = {Agarwal, Sameer and Mallick, Satya P and Kriegman, David and Belongie,
	Serge},
  title = {{On refractive optical flow}},
  booktitle = {Proc. ECCV' 04},
  year = {2004},
  volume = {3022},
  pages = {483--494},
  publisher = {Springer},
  abstract = {This paper presents a novel generalization of the optical flow equation
	to the case of refraction, and it describes a method for recovering
	the refractive structure of an object from a video sequence acquired
	as the background behind the refracting object moves. By structure
	here we mean a representation of how the object warps and attenuates
	(or amplifies) the light passing through it. We distinguish between
	the cases when the background motion is known and unknown. We show
	that when the motion is unknown, the refractive structure can only
	be estimated up to a six-parameter family of solutions without additional
	sources of information. Methods for solving for the refractive structure
	are described in both cases. The performance of the algorithm is
	demonstrated on real data, and results of applying the estimated
	refractive structure to the task of environment matting and compositing
	are presented.},
  file = {:C$\backslash$:/Users/amitibo/Documents/Mendeley Desktop//Agarwal et al. - 2004 - On refractive optical flow.pdf:pdf},
  isbn = {9783540219835,},
  url = {http://www.springerlink.com/index/xde80cfqjgdc28un.pdf}
}

